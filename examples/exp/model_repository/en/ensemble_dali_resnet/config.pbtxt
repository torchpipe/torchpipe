# https://github.com/triton-inference-server/dali_backend/tree/main/docs/examples/inception_ensemble/model_repository/inception_graphdef

name: "ensemble_dali_resnet"
platform: "ensemble"
max_batch_size: 4
input [
  {
    name: "INPUT"
    data_type: TYPE_UINT8
    dims: [ -1 ]
    allow_ragged_batch: true
  }
]
output [
  {
    name: "OUTPUT"
    data_type: TYPE_FP32
    dims: [ 1000 ]
  }
]
ensemble_scheduling {
  step [
    {
      model_name: "dali"
      model_version: -1
      input_map {
        key: "DALI_INPUT_0"
        value: "INPUT"
      }
      output_map {
        key: "DALI_OUTPUT_0"
        value: "preprocessed_image"
      }
    },
    {
      model_name: "resnet_trt"
      model_version: -1
      input_map {
        key: "input"
        value: "preprocessed_image"
      }
      output_map {
        key: "output_1"
        value: "OUTPUT"
      }
    }
  ]
}