name: "resnet_trt"
platform: "tensorrt_plan"
max_batch_size: 8
input [
  {
    name: "input"
    data_type: TYPE_FP32

    dims: [ 3, 224, 224 ]

  }
]
output [
  {
    name: "output_0"
    #data_type: TYPE_FP32
    dims: [ 1000 ]
  }
]

dynamic_batching {
  max_queue_delay_microseconds: 2000
}

instance_group [
  {
    count: 2
    kind: KIND_GPU
  }
]